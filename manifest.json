{

  "manifest_version": 2,
  "name": "Invidious Redirect",
  "version": "1.8",
  "description": "Redirects all YouTube links to Invidious. Previously hooktube-redirect.",
  "permissions": [
    "storage"
  ],
  "applications": {
      "gecko": {
        "id": "invidious-redirect@metlis",
        "strict_min_version": "60.0"
      }
  },
  "icons": {
    "48": "icons/hooktube-48.ico"
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "run_at": "document_start",
      "js": ["makehook.js"]
    }
  ]

}
