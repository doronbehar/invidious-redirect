{

  "manifest_version": 2,
  "name": "Invidious Redirect",
  "version": "1.8",
  "description": "Redirects all YouTube links to Invidious. Previously hooktube-redirect.",
  "permissions": [
    "storage",
    "*://*.youtube.com/*",
    "*://*.youtube-nocookie.com/*",
    "*://*.youtu.be/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "applications": {
      "gecko": {
        "id": "invidious-redirect@doronbehar",
        "strict_min_version": "60.0"
      }
  },
  "icons": {
    "48": "icons/hooktube-48.ico"
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "background": {
    "scripts": ["background.js"]
  }
}
